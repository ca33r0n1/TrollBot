debug: false
bot:
  # Name of your bot
  name: "Change Bot Name"

  # Current version of your bot
  version: "2.9 - NSL"

# Application - API Settings
api:
  # Twitch Application Client Id
  twitch_client_id: MakeThisInTwitch

  # Twitch Application Client Secret
  twitch_client_secret: MakeThisInTwitch

# Credentials
credentials:
  # OAuth Token for IRC USer (You can get yours here: http://twitchapps.com/tmi/)
  irc: oauth:Something here

# Channels - The bot will connect to all listed channels
channels:
  - OwLookAStreamer